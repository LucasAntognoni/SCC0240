<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>

<MenuBar prefHeight="40.0" prefWidth="150.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MainMenuController">
   <menus>
      <Menu mnemonicParsing="false" text="Menu Principal">
        <items>
          <MenuItem fx:id="managerView" mnemonicParsing="false" onAction="#handleChangeToManagerView" text="Gerenciar Atletas" />
            <MenuItem fx:id="reportView" mnemonicParsing="false" onAction="#handleChangeToReportView" text="Gerar Relatorios" />
            <MenuItem fx:id="aboutView" mnemonicParsing="false" onAction="#handleChangeToAboutView" text="Sobre" />
        </items>
      </Menu>
   </menus>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</MenuBar>
